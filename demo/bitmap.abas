10 HOME
20 F$="IMAGE.BITMAP"
30 PRINT CHR$(4);"BLOAD ";F$;",A$6000":PRINT
40 A=24567: REM $6000
50 W=16: H=16
60 I=7
70 FOR Y = 1 TO H
80   FOR X = 1 TO W
90     HTAB X: VTAB Y
100    IF I = 7 THEN D = PEEK(A): A = A + 1
110    B = INT(D / (2^I))
120    IF B = 1 THEN PRINT "@";: D = D - (2^I)
130    IF B = 0 THEN PRINT ".";
140    I = I - 1: IF I < 0 THEN I = 7
150   NEXT X
160 NEXT Y
170 END
