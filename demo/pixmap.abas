10 HOME
11 PRINT "PIXMAP VIEWER"
12 INPUT "FILENAME:";F$
13 INPUT "WIDTH:";W
14 INPUT "HEIGHT:";H
20 HOME
30 A=24576: REM $6000
40 PRINT CHR$(4);"BLOAD ";F$;",A";A:PRINT
50 HX$="0123456789ABCDEF"
100 FOR Y = 1 TO H
110   FOR X = 1 TO W
120     D = PEEK(A): A = A + 1
130     HI = INT(D / 16): LO = D - HI * 16
140     HTAB X * 2: VTAB Y
150     PRINT MID$(HX$, HI + 1, 1) + MID$(HX$, LO + 1, 1);
160   NEXT X
170 NEXT Y
180 END
