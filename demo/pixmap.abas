10 HOME
20 F$="IMAGE.PIXMAP"
30 PRINT CHR$(4);"BLOAD ";F$;",A$6000":PRINT
40 A=24567: REM $6000
50 W=16: H=16
60 FOR Y = 1 TO H
70   FOR X = 1 TO W
80     D = PEEK(A): A = A + 1
90     HTAB X: VTAB Y
100    R = D - (INT(D/16)*16)
110    PRINT MID$(".123456789ABCDEF", R+1, 1)
120   NEXT X
130 NEXT Y
140 END
